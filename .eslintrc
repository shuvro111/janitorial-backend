{
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "error"
  }
}
